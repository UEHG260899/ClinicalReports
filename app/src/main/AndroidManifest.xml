<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.clinicalreports" >
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ClinicalReports" >
        <activity
            android:name=".DrawerAlumno"
            android:label="@string/title_activity_alumno_drawer"
            android:theme="@style/Theme.ClinicalReports.NoActionBar" >
        </activity>
        <activity
            android:name=".DrawerMaestro"
            android:label="@string/title_activity_drawer_maestro"
            android:theme="@style/Theme.ClinicalReports.NoActionBar" />
        <activity android:name=".RegistroActivity" />
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:authorities="com.example.clinicalreports"
            android:name="androidx.core.content.FileProvider"
            android:exported="false"
            android:grantUriPermissions="true">

            <meta-data android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/filepath"/>

        </provider>
    </application>

</manifest>